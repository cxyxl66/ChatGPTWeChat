.container{
  height : 100vh;
}

.partOne {
  display: flex;
  justify-content: center;
  background-color: thistle;
  width: 100%;
  height: 500rpx;
}

.userinfo {
  display: flex;
  width:100%;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 30rpx;
  border-radius: 50%;
  box-shadow: 0px 0px 18px 0px rgba(5, 5, 5, 0.15);
}
.userinfo-nickname {
  height: 128rpx;
  font-weight: bold;
  color:white;
}


.partTwo {
  width: 80%;
  margin-left: 10%;
  margin-top: -50px;
  height: 800rpx;
  box-shadow: 0px 0px 18px 0px rgba(5, 5, 5, 0.15);
  border-radius: 10px;
  background-color: white;
}

